{
    "name": "penguins-eggs",
    "description": "Perri's Brewery edition: remaster your system and distribuite it",
    "version": "9.3.22",
    "author": "Piero Proietti @pieroproietti",
    "bin": {
        "eggs": "bin/run"
    },
    "bugs": "https://github.com/pieroproietti/penguins-eggs/issues",
    "dependencies": {
        "@oclif/core": "^1.21.0",
        "@oclif/plugin-autocomplete": "1.3.8",
        "@oclif/plugin-help": "^5.1.23",
        "@oclif/plugin-not-found": "^2.3.15",
        "@oclif/plugin-version": "^1.1.3",
        "@oclif/plugin-warn-if-update-available": "^2.0.19",
        "@oclif/test": "^2.2.21",
        "axios": "^1.2.1",
        "chalk": "^4.1.2",
        "ink": "^3.2.0",
        "ink-big-text": "^1.2.0",
        "ink-gradient": "^2.0.0",
        "inquirer": "^8.2.4",
        "js-yaml": "^4.1.0",
        "mustache": "^4.2.0",
        "netmask": "^2.0.2",
        "node-static": "^0.7.11",
        "react": "^17.0.2",
        "shelljs": "0.8.5",
        "tftp": "^0.1.2",
        "tslib": "^2.4.1",
        "typescript": "^4.9.4"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.1",
        "@types/chai": "^4.3.4",
        "@types/ini": "^1.3.31",
        "@types/ink-big-text": "^1.2.1",
        "@types/ink-gradient": "^2.0.1",
        "@types/inquirer": "^9.0.3",
        "@types/js-yaml": "^4.0.5",
        "@types/mocha": "^10.0.1",
        "@types/mustache": "^4.2.2",
        "@types/netmask": "^1.0.30",
        "@types/node": "^18.11.18",
        "@types/node-static": "^0.7.7",
        "@types/react": "^18.0.26",
        "@types/shelljs": "^0.8.11",
        "@typescript-eslint/parser": "^5.47.1",
        "chai": "^4.3.7",
        "eslint": "^7.32.0",
        "eslint-config-oclif": "^4.0.0",
        "eslint-config-oclif-typescript": "^1.0.3",
        "globby": "^11.1.0",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "oclif-pnpm": "^3.4.3-1",
        "perrisbrewery": "^9.3.13-2",
        "ts-node": "^10.9.1"
    },
    "peerDependencies": {
        "@typescript-eslint/parser": "^5.0.0",
        "@typescript-eslint/plugin": "^5.0.0"
    },
    "dirname": "eggs",
    "engines": {
        "node": ">=16.0.0"
    },
    "files": [
        ".oclif.manifest.json",
        "/addons",
        "/assets",
        "/bin",
        "/conf",
        "/dist",
        "/ipxe",
        "/manpages",
        "mkinitcpio",
        "/scripts"
    ],
    "homepage": "https://penguins-eggs.net/",
    "keywords": [
        "oclif",
        "refracta",
        "remastersys",
        "systemback"
    ],
    "license": "MIT",
    "main": "./dist/index.js",
    "oclif": {
        "bin": "eggs",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-autocomplete",
            "@oclif/plugin-help",
            "@oclif/plugin-not-found",
            "@oclif/plugin-warn-if-update-available",
            "@oclif/plugin-version"
        ],
        "topicSeparator": " ",
        "additionalHelpFlags": [
            "-h"
        ],
        "additionalVersionFlags": [
            "-v"
        ]
    },
    "repository": "https://github.com/pieroproietti/penguins-eggs",
    "scripts": {
        "build": "tsc -p .",
        "deb": "oclif pack:deb && pb deb ./",
        "docs": "rm docs -rf && npx typedoc src --out docs",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "lint-and-fix": "eslint . --ext .ts --fix --config .eslintrc",
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm dist -rf && tsc -b && oclif manifest && oclif readme",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "tarballs": "oclif pack:tarballs --root=. --targets=linux-x64 --no-xz",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md"
    },
    "shortName": "eggs"
}
