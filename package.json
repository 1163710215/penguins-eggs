{
    "name": "penguins-eggs",
    "description": "Perri's Brewery edition: remaster your system and distribuite it",
    "version": "9.7.6",
    "author": "Piero Proietti @pieroproietti",
    "bin": {
        "eggs": "bin/run"
    },
    "type": "commonjs",
    "bugs": "https://github.com/pieroproietti/penguins-eggs/issues",
    "dependencies": {
        "@oclif/core": "^1.26.2",
        "@oclif/plugin-autocomplete": "1.3.8",
        "@oclif/plugin-help": "^5.2.20",
        "@oclif/plugin-version": "1.2.1",
        "@oclif/plugin-warn-if-update-available": "^2.1.1",
        "@oclif/test": "^2.5.6",
        "axios": "^1.6.8",
        "chalk": "^4.1.2",
        "etrick": "0.0.3",
        "ink": "^3.2.0",
        "ink-big-text": "^1.2.0",
        "ink-gradient": "^2.0.0",
        "inquirer": "^8.2.6",
        "js-yaml": "^4.1.0",
        "mustache": "^4.2.0",
        "netmask": "^2.0.2",
        "node-static": "^0.7.11",
        "react": "^18.3.1",
        "shelljs": "0.8.5",
        "systeminformation": "^5.22.9",
        "tftp": "^0.1.2",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.6",
        "@types/chai": "^4.3.16",
        "@types/ini": "^1.3.34",
        "@types/ink-big-text": "^1.2.4",
        "@types/ink-gradient": "^2.0.4",
        "@types/inquirer": "^9.0.7",
        "@types/js-yaml": "^4.0.9",
        "@types/mocha": "^10.0.6",
        "@types/mustache": "^4.2.5",
        "@types/netmask": "^1.0.30",
        "@types/node": "^18.19.33",
        "@types/node-static": "^0.7.11",
        "@types/react": "^18.3.2",
        "@types/shelljs": "^0.8.15",
        "@typescript-eslint/parser": "^5.62.0",
        "chai": "^4.4.1",
        "eslint": "^7.32.0",
        "eslint-config-oclif": "^4.0.0",
        "eslint-config-oclif-typescript": "^1.0.3",
        "globby": "^11.1.0",
        "mocha": "^10.4.0",
        "nyc": "^15.1.0",
        "oclif-pnpm": "3.4.3-1",
        "perrisbrewery": "9.5.9",
        "ts-node": "^10.9.2"
    },
    "dirname": "eggs",
    "engines": {
        "node": ">=16"
    },
    "files": [
        ".oclif.manifest.json",
        "/addons",
        "/assets",
        "/bin",
        "/conf",
        "/dist",
        "/eui",
        "/ipxe",
        "/manpages",
        "/mkinitcpio",
        "/scripts"
    ],
    "homepage": "https://penguins-eggs.net/",
    "keywords": [
        "oclif",
        "pxe",
        "refracta",
        "remastersys",
        "systemback"
    ],
    "license": "MIT",
    "main": "./dist/index.js",
    "oclif": {
        "bin": "eggs",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-autocomplete",
            "@oclif/plugin-help",
            "@oclif/plugin-warn-if-update-available",
            "@oclif/plugin-version"
        ],
        "topicSeparator": " ",
        "additionalHelpFlags": [
            "-h"
        ],
        "additionalVersionFlags": [
            "-v"
        ]
    },
    "repository": "https://github.com/pieroproietti/penguins-eggs",
    "scripts": {
        "build": "tsc -p .",
        "deb": "oclif pack:deb && pb deb ./",
        "docs": "rm docs -rf && npx typedoc src --out docs",
        "lint-and-fix": "eslint . --ext .ts --fix --config .eslintrc",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "manifest": "oclif manifest",
        "readme": "oclif readme",
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "_prepack": "rm dist -rf && tsc -b && oclif manifest && oclif readme",
        "prepack": "rm dist -rf && tsc -b && oclif manifest",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "tarballs": "oclif pack:tarballs --root=. --targets=linux-x64 --no-xz",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md"
    },
    "shortName": "eggs"
}
