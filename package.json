{
    "name": "penguins-eggs",
    "shortName": "eggs",
    "description": "Perri's Brewery edition: remaster your system and distribuite it",
    "version": "16.13.0",
    "author": "Piero Proietti @pieroproietti",
    "bin": {
        "eggs": "bin/run"
    },
    "homepage": "https://penguins-eggs.net/",
    "repository": "https://github.com/pieroproietti/penguins-eggs",
    "bugs": "https://github.com/pieroproietti/penguins-eggs/issues",
    "dependencies": {
        "@oclif/command": "^1.8.13",
        "@oclif/config": "^1.18.2",
        "@oclif/plugin-autocomplete": "0.3.0",
        "@oclif/plugin-help": "^3.2.17",
        "@oclif/plugin-not-found": "^1.2.6",
        "@oclif/plugin-warn-if-update-available": "^2.0.2",
        "@typescript-eslint/parser": "^5.7.0",
        "axios": "^0.24.0",
        "cfonts": "^2.10.0",
        "chalk": "^4.1.0",
        "ink": "^3",
        "ink-big-text": "^1.2.0",
        "ink-gradient": "^2.0.0",
        "inquirer": "^8",
        "js-yaml": "^4",
        "mustache": "^4.2.0",
        "pjson": "github:pieroproietti/pjson",
        "react": "^16.14.0",
        "shelljs": "github:shelljs/shelljs",
        "tslib": "^1.14.1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.9",
        "@oclif/test": "^1.2.9",
        "@types/chai": "^4.3.0",
        "@types/ink-big-text": "^1.2.0",
        "@types/ink-gradient": "^2.0.1",
        "@types/inquirer": "^7.3.1",
        "@types/js-yaml": "^4.0.1",
        "@types/mocha": "^5.2.7",
        "@types/mustache": "^4.1.1",
        "@types/node": "^10.17.60",
        "@types/react": "^17.0.5",
        "@types/shelljs": "^0.8.8",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "chai": "^4.3.4",
        "eslint": "^7.32.0",
        "eslint-config-oclif": "^3.1.2",
        "eslint-config-oclif-typescript": "^0.2.0",
        "globby": "^10.0.2",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "perrisbrewery": "./vendors/perrisbrewery",
        "prettier": "^2.3.0",
        "ts-node": "^8.10.2",
        "typedoc": "^0.20.36",
        "typescript": "^3.9.10"
    },
    "engines": {
        "node": ">12.0.0"
    },
    "files": [
        "/assets",
        "/addons",
        "/assistant",
        "/bin",
        "/conf",
        "/lib",
        "/manpages",
        "/oclif.manifest.json",
        "/scripts",
        "/templates"
    ],
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "eggs",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-autocomplete",
            "@oclif/plugin-not-found",
            "@oclif/plugin-warn-if-update-available"
        ]
    },
    "scripts": {
        "docs": "rm docs -rf && npx typedoc src --out docs",
        "build": "tsc -p .",
        "deb": "oclif-dev pack:deb && npx pb -m",
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm lib -rf && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "lint-and-fix": "eslint . --ext .ts --fix --config .eslintrc",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
    },
    "types": "lib/index.d.ts"
}
