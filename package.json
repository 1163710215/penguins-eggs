{
  "name": "penguins-eggs",
  "description": "Perri's Brewery edition: remaster your system and distribuite it",
  "version": "9.7.8",
  "author": "Piero Proietti @pieroproietti",
  "bin": {
    "eggs": "./bin/run.js"
  },
  "type": "commonjs",
  "bugs": "https://github.com/pieroproietti/penguins-eggs/issues",
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-autocomplete": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-version": "^2",
    "@oclif/plugin-warn-if-update-available": "^3",
    "@oclif/test": "^4",
    "axios": "^1.7.2",
    "chalk": "^4.1.2",
    "etrick": "0.0.3",
    "ink": "^3.2.0",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0",
    "netmask": "^2.0.2",
    "node-static": "^0.7.11",
    "react": "^18.3.1",
    "shelljs": "0.8.5",
    "systeminformation": "^5.22.9",
    "tftp": "^0.1.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.6",
    "@types/chai": "^4.3.16",
    "@types/ini": "^4.1.0",
    "@types/ink-big-text": "^1.2.4",
    "@types/ink-gradient": "^2.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.6",
    "@types/mustache": "^4.2.5",
    "@types/netmask": "^2.0.5",
    "@types/node": "^20.12.12",
    "@types/node-static": "^0.7.11",
    "@types/react": "^18.3.3",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/parser": "^7.10.0",
    "chai": "^5.1.1",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "globby": "^11.1.0",
    "mocha": "^10",
    "nyc": "^15.1.0",
    "oclif": "^4",
    "perrisbrewery": "../perrisbrewery/",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "dirname": "eggs",
  "engines": {
    "node": ">=18"
  },
  "files": [
    ".oclif.manifest.json",
    "/addons",
    "/assets",
    "/bin",
    "/conf",
    "/dist",
    "/eui",
    "/ipxe",
    "/manpages",
    "/mkinitcpio",
    "/scripts"
  ],
  "homepage": "https://penguins-eggs.net/",
  "keywords": [
    "oclif",
    "pxe",
    "refracta",
    "remastersys",
    "systemback"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "oclif": {
    "bin": "eggs",
    "commands": "./dist/commands",
    "dirname": "eggs",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-version"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "https://github.com/pieroproietti/penguins-eggs",
  "scripts": {
    "build": "tsc -p .",
    "deb": "tsc -p . && pb deb",
    "docs": "rm docs -rf && npx typedoc src --out docs",
    "lint-and-fix": "eslint . --ext .ts --fix --config .eslintrc",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "manifest": "oclif manifest",
    "readme": "oclif readme",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "_prepack": "rm dist -rf && tsc -b && oclif manifest && oclif readme",
    "prepack": "rm dist -rf && tsc -b && oclif manifest",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "tarballs": "oclif pack:tarballs --root=. --targets=linux-x64 --no-xz",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "shortName": "eggs"
}